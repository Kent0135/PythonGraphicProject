import numpy as np
array = np.random.randint(1, 100, size=(5, 5), dtype=int)
print("The entire array is ", array)
print("The number from the 2nd row, 3rd column is ", array[1, 2])
summer = np.sum(array)
print("The sum of the array is ", summer)
print("The means of each rows are", np.mean(array, axis=1))
print("The maximum values in each column are", np.max(array, axis=0))
